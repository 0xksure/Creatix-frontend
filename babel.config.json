{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "extensions": [".js", ".jsx", ".scss"],
        "alias": {
          "root": ".",
          "src": "/src",
          "test": "./test",
          "reducers": "./src/reducers"
        }
      }
    ]
  ]
}
