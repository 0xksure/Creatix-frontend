{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "NODE_ENV=prod webpack --config webpack.prod.js --display-error-details",
    "test": "jest",
    "lint": "eslint src/**/*.{js,ts,tsx} --report-unused-disable-directives",
    "autolint": "eslint --fix src/**/*.{js,ts,tsx} --report-unused-disable-directives",
    "precommit": "NODE_ENV=prod lint-staged",
    "create-env": "printenv > .env"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-typescript": "^7.10.4",
    "@svgr/webpack": "^5.1.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^9.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "connected-react-router": "^6.8.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^4.3.0",
    "dotenv-expand": "^5.1.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "file-loader": "^6.1.0",
    "formik": "^2.1.2",
    "foundation-sites": "^6.6.1",
    "fs-extra": "^9.0.1",
    "history": "^5.0.0",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "is-wsl": "^2.1.1",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^26.4.0",
    "jest-watch-typeahead": "^0.6.1",
    "js-cookie": "^2.2.1",
    "json-loader": "^0.5.7",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.21",
    "lodash-move": "^1.1.1",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^4.0.2",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^5.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-datepicker": "^3.1.3",
    "react-dev-utils": "^10.0.0",
    "react-dom": "^16.12.0",
    "react-ga": "^3.1.2",
    "react-gtm-module": "^2.0.8",
    "react-helmet-async": "^1.0.4",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "react-redux-form": "^1.16.14",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.1.0",
    "react-router-redux": "^4.0.8",
    "react-spring": "^8.0.27",
    "react-svg-loader": "^3.0.3",
    "react-uid": "^2.2.0",
    "react-use-gesture": "^7.0.16",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "^1.15.0",
    "sass-loader": "^8.0.2",
    "sitemap-webpack-plugin": "^0.8.1",
    "source-map-support": "^0.5.19",
    "strip-ansi": "^6.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.1",
    "ts-pnp": "^1.1.5",
    "typescript": "^4.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.1.4",
    "yup": "^0.29.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "babel-eslint": "^10.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^26.4.2",
    "path": "^0.12.7",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "redux-devtools": "^3.5.0"
  }
}
